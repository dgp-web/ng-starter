{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "rootDir": ".",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "module": "es2015",
        "paths": {},
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2016",
            "es2016.array.include",
            "dom"
        ]
    },
    "exclude": [
        "bin",
        "wwwroot",
        "node_modules"
    ]
}
